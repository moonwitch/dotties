! Color Scheme
#include ".config/colors/gruvbox.dark"
#include ".config/colors/gruvbox-urxvt.xresources"

! URXVT Config
URxvt.geometry:     72x18
URxvt*font:         xft:Hack Nerd Font Mono:style=Regular:size=12:antialias=true
URxvt*letterSpace:  0
URxvt.scrollBar:    false
URxvt.depth:        32s
URxvt.iso14755:     true
URxvt.iso14755_52:  false

URxvt.url-launcher:   /usr/bin/chromium
URxvt.underlineURLs:  True

!TABS
URxvt.perl-ext-common : default,tabbed,selection-to-clipboard
URxvt.tabbed.tabbar-fg: 2
URxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.tab-fg:    3
URxvt.tabbed.tab-bg:    0

Xft.dpi:       96
Xft.antialias: true
Xft.hinting:   true
Xft.rgba:      rgb
Xft.autohint:  false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault

xterm*font:         xft:Hack Nerd Font Mono:style=Regular:size=12:antialias=true
